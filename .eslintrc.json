{
  "root": true,
  "rules": {
    "prettier/prettier": ["error", {"trailingComma": "es5", "semi": false}],
    "no-cond-assign": "warn",
    "no-console": "warn",
    "no-constant-condition": "error",
    "no-control-regex": "error",
    "no-debugger": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-empty-character-class": "error",
    "no-empty": "error",
    "no-ex-assign": "error",
    "no-extra-boolean-cast": "error",
    "no-extra-parens": "off",
    "no-func-assign": "error",
    "no-inner-declarations": "off",
    "no-invalid-regexp": "error",
    "no-irregular-whitespace": "error",
    "no-negated-in-lhs": "error",
    "no-obj-calls": "error",
    "no-regex-spaces": "error",
    "no-sparse-arrays": "error",
    "no-unexpected-multiline": "error",
    "no-unreachable": "error",
    "use-isnan": "error",
    "valid-jsdoc": "off",
    "valid-typeof": "error",
    "accessor-pairs": "off",
    "block-scoped-var": "warn",
    "complexity": [
      "error",
      15
    ],
    "consistent-return": "error",
    "default-case": "error",
    "dot-notation": [
      "warn"
    ],
    "eqeqeq": [
      "error",
      "smart"
    ],
    "guard-for-in": "error",
    "no-caller": "error",
    "no-div-regex": "error",
    "no-else-return": "off",
    "no-eval": "error",
    "no-extend-native": "error",
    "no-extra-bind": "warn",
    "no-fallthrough": "warn",
    "no-floating-decimal": "error",
    "no-implicit-coercion": "off",
    "no-implied-eval": "error",
    "no-iterator": "error",
    "no-labels": "error",
    "no-lone-blocks": "off",
    "no-loop-func": "error",
    "no-magic-numbers": "off",
    "no-multi-str": "warn",
    "no-native-reassign": "error",
    "no-new-func": "error",
    "no-new-wrappers": "error",
    "no-new": "warn",
    "no-octal-escape": "error",
    "no-octal": "error",
    "no-param-reassign": "error",
    "no-process-env": "off",
    "no-proto": "error",
    "no-redeclare": "warn",
    "no-return-assign": "warn",
    "no-script-url": "warn",
    "no-self-compare": "error",
    "no-sequences": "warn",
    "no-throw-literal": "error",
    "no-unused-expressions": [
      "error",
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-useless-call": "error",
    "no-useless-concat": "error",
    "no-void": "warn",
    "no-warning-comments": "warn",
    "no-with": "error",
    "radix": "error",
    "vars-on-top": "off",
    "yoda": "off",
    "no-shadow": "warn",
    "func-names": "warn",
    "linebreak-style": [
      "error",
      "unix"
    ],
    "max-depth": [
      "warn",
      4
    ],
    "max-params": [
      "warn",
      3
    ],
    "new-cap": "error",
    "no-bitwise": "error",
    "no-lonely-if": "warn",
    "no-plusplus": "warn",
    "no-use-before-define": [
      "error",
      "nofunc"
    ],
    "spaced-comment": [
      "error",
      "always",
      {
        "markers": [
          "="
        ]
      }
    ],
    "constructor-super": "error",
    "no-const-assign": "error",
    "no-var": "error",
    "object-shorthand": "warn",
    "prefer-const": "warn",
    "prefer-spread": "warn",
    "prefer-template": "warn",
    "react/react-in-jsx-scope": "warn",
    "react/jsx-uses-react": "error",
    "react/prop-types": "error",
    "react/sort-comp": [
      "warn",
      {
        "order": [
          "lifecycle",
          "everything-else",
          "/^handle.+$/",
          "/^get.+$/",
          "/^render.+$/",
          "render"
        ]
      }
    ],
    "react/jsx-key": "error",
    "react/jsx-no-bind": [
      "error",
      {
        "allowArrowFunctions": true
      }
    ],
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-no-undef": "error",
    "react/jsx-uses-vars": "error",
    "react/jsx-pascal-case": "error",
    "react/no-multi-comp": "off",
    "react/prefer-es6-class": [
      "error",
      "always"
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "mocha": true,
    "node": true,
    "builtin": true,
    "jquery": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "prettier",
    "react"
  ],
  "parser": "babel-eslint",
  "globals": {
    "Alt": true,
    "ApplePaySession": true,
    "AvatarEditorModal": true,
    "CodeMirror": true,
    "Helpdesk": true,
    "I18n": true,
    "INTERFACES": true,
    "Ladda": true,
    "Modernizr": true,
    "PersonEditorModal": true,
    "Plaid": true,
    "Stripe": true,
    "Turbolinks": true,
    "_": true,
    "sweetAlert": true,
    "Spinner": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "prettier/react"
  ]
}
